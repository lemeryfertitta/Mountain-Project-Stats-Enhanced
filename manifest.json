{
  "homepage_url": "https://github.com/lemeryfertitta/Mountain-Project-Stats-Enhanced",
  "manifest_version": 3,
  "name": "Mountain Project Stats Enhanced",
  "version": "1.1",
  "description": "Dated and sortable grades, stars, and todos for Mountain Project routes.",
  "icons": {
    "16": "media/icon-16.png",
    "32": "media/icon-32.png",
    "48": "media/icon-48.png",
    "128": "media/icon-128.png"
  },
  "content_scripts": [
    {
      "world": "MAIN",
      "js": ["script.js"],
      "matches": ["https://www.mountainproject.com/route/stats/*"],
      "run_at": "document_start"
    }
  ],
  "permissions": []
}
